<div class="comparisonTable page-width">
  <div class="tablemodule">
    <div class="heading-content">
      {% if section.settings.section_heading != nill %}
        <h2>{{ section.settings.section_heading }}</h2>
      {% endif %}
      {% if section.settings.section_description != nill %}
        <p>{{ section.settings.section_description }}</p>
      {% endif %}
      
    </div>
    <div class="bm-table-container">
      <table class="tablemodule-table" cellspacing="0" cellpadding="0">
        <tbody>
          <tr class="tablemodule-imagerows">
            {% if section.settings['tablemodule-image1'] != blank
              or section.settings['tablemodule-text1'] != blank
            %}
              <th class="tablemodule-image cell_Primary_color pdp-module">
                <div class="tableheader_wrapper">
                  {% if section.settings['tablemodule-image1'] != blank %}
                    <div class="icon">
                      <img
                        src="{{ section.settings.tablemodule-image1 | img_url: 'master' }}"
                      >
                    </div>
                  {% endif %}
                  {% if section.settings['tablemodule-text1'] != blank %}
                    <p>{{ section.settings['tablemodule-text1'] }}</p>
                  {% endif %}
                </div>
              </th>
            {% endif %}
