{% style %}
      .comparisonTable .heading-content h2{
        font-size:{{section.settings.heading_size}}px;
        text-align: {{section.settings.text_align}};
        margin:0;
        margin-bottom:16px;
        font-style: normal;
        font-weight: 700;
        line-height: 140%;
      }
      .comparisonTable .heading-content p{
        font-size:{{section.settings.text_size}}px;
         margin-bottom:40px !important;
        font-style: normal;
        font-weight: 400;
        line-height: 140%;
        text-align: {{section.settings.text_align}};
      }
      #shopify-section-{{ section.id }}{
          background-color: {{section.settings.section_background}};
          padding:{{section.settings.section_padding}};
      }
      #shopify-section-{{ section.id }} .tablemodule-valuecell.cell_Primary_color,
      #shopify-section-{{ section.id }} .tablemodule-image.cell_Primary_color{
          background-color: {{section.settings.cell_Primary_color}};
          border-bottom-color: {{ section.settings.selected_border_color }};
      }
      #shopify-section-{{ section.id }} .tablemodule-valuecell.cell_Primary_color p{
        color: {{ section.settings.Selected_content_color }};
      }
      #shopify-section-{{ section.id }} .tablemodule table td, #shopify-section-{{ section.id }} .tablemodule-row th{
        border-bottom: 1px solid {{ section.settings.unselected_border_color }};
        border-right:1px solid {{ section.settings.unselected_border_color }};
      }
      /* #shopify-section-{{ section.id }}  .tablemodule table{
       border-top: 1px solid {{ section.settings.unselected_border_color }};
      } */
      table.tablemodule-table tr{
        height:70px;
      }
     .tablemodule-valuecell {
         padding: 10px;
         min-width:193px;
      }
    .tablemodule .icon {
        max-width: 45px;
        margin-left: auto;
         margin-right: auto;
        height: 80px;
        display: flex;
        align-items: center;
      }
       th.tablemodule-image {
          padding: 10px;
      }

      #shopify-section-{{ section.id }} th.tablemodule-keyhead {
              padding: 10px;
      }
    #shopify-section-{{ section.id }} th.tablemodule-image:last-of-type, #shopify-section-{{ section.id }} .tablemodule table td:last-of-type {
       border-right: 1px solid {{ section.settings.unselected_border_color }};
    }
      .tablemodule-valuecell span {
              width: 22px;
              margin: auto;
              display: block;
      }
      
      .tablemodule-valuecell .our-proprietary-image{
        width: 125px;
      }
      .tablemodule-valuecell .our-proprietary-image+.mobile_icon{
        width:125px;
        display:none;
      }
      .tablemodule-valuecell:nth-child(5) .our-proprietary-image{
        width:82px;
      }

     .tablemodule-valuecell span img {
              width: 100%;
            display:block;
          }
           #shopify-section-{{ section.id }} .tablemodule-image p {
              font-size: 18px;
              font-style: normal;
              font-weight: 700;
              line-height: 24px; /* 133.333% */
              color: {{ section.settings.unselected_content_color }};
            
          }
      .tablemodule table td:last-of-type, th.tablemodule-image:last-of-type{
            padding-right:30px;
          }
        th.tablemodule-keyhead{
            padding-left:25px !important;
            
          }
        th.tablemodule-keyhead{
              max-width: 280px;
        }
        .tableheader_wrapper{
           align-items: center;
          }
     /* #shopify-section-{{ section.id }} .tablemodule-valuecell p, #shopify-section-{{ section.id }} .tablemodule-keyhead .a-color-secondary {
       color: {{ section.settings.unselected_content_color }};
    } */
          .tablemodule .tableheader_wrapper {
              display: flex;
              flex-direction: column;
              gap: 25px;
            align-items:center;
          }

          .tablemodule-row .tablemodule-keyhead {
              text-align: left;
          }

          .tablemodule-keyhead .a-color-secondary {
              
              font-size: 16px;
              font-style: normal;
              font-weight: 400;
              line-height: 20px; /* 125% */
          }

          .cell_secondry_color {
              background-color: aliceblue;
          }

          .cell_transparent_color {
              background-color: transparent;
          }


    /*   slider for table */
    .bm-table-container .tablemodule-table {
        margin-bottom: 0px;
    }
    .bm-table-container {
        overflow: auto;
        border-image: initial;
    }
    .comparisonTable .bm-seeMore{
        margin-top: -47px;
        max-width: 100px;
    }
     .arrow_text{
           font-family: 'Futura Md BT';
    }
    tr.tablemodule-imagerows> th {
       border-bottom: 1px solid #E5E5E5;
      border-right: 1px solid #E5E5E5;
     }
        /*   end slider table */
        @media(min-width:1140px){
          
    tr.tablemodule-imagerows> th {
          position: sticky;
          top: 101px;
          box-sizing: border-box;
          background: #ffffff;
      }
    }
    @media(max-width:1140px){
      .tablemodule-table>tbody>tr>th:nth-child(1) {
        position: sticky !important;
        background: rgb(255, 255, 255);
        left: 0px;
        z-index: 1;
    }
    }
     @media(min-width:1140px){
      .bm-table-container {
        overflow: visible !important;
    }
     }
    .tableheader_wrapper p{
        min-height:38px;
      }
  @media (max-width:740px) {
    .tablemodule-valuecell .our-proprietary-image+.mobile_icon{
        width:39px;
        display:block;
      }
    .tablemodule-valuecell .our-proprietary-image{
        display:none;
      }
    .tablemodule-valuecell{
             min-width: 116px;
    }
    .comparisonTable {
    padding-bottom: 2em;
  }
    .tablemodule .icon{
        height: 55px;
    }
    .tablemodule .tableheader_wrapper{
      gap: 10px;
    }
   h2, h2+p{
         text-align: left !important;
    margin-top: 10px;
    margin-bottom: 30px;
    }
     #shopify-section-{{ section.id }} h2+p{
       margin-bottom: 20px;
     }
  .tablemodule .slideBack,  .tablemodule .slidenext{
          max-width: 50px;
  
    }
    .tablemodule .slideBack{
        margin-right: 17px;
    }
    .bm-table-container {
        margin-right: -20px;
    }
    .comparisonTable h2 +p{
      line-height: 17px;
    }
    #shopify-section-{{ section.id }}{
       padding-top: 34px;
    padding-bottom: 15px;
    }
    .tablemodule-table>tbody>tr>th:nth-child(1) {
            position: sticky;
            background: rgb(255, 255, 255);
            left: 0px;
        }

     .bm-hide-tableData{
              display: none;
            }
      .tablemodule .slide-button {
        display: flex;
        align-items:flex-start;
        justify-content: center;
        margin-bottom: 20px;
        margin-top: 20px;
        gap: 5px;
        max-width: 188px;
        margin-left: auto;
    }
    .tablemodule .slide-button span {
        display: inline-block;
        font-size: 20px;
        cursor: pointer;
        width: 100%;
      line-height:16px;
    }
    }
    @media(min-width:741px){
        .comparisonTable .bm-seeMore{
          display:none;
          position: sticky;
          left: 11px;
        }
        .tablemodule .slide-button{
          display:none;
    }
  }

  #shopify-section-{{ section.id }} .bm-table-container {
       border-left: 1px solid {{ section.settings.unselected_border_color }};
  }
{% endstyle %}
{% schema %}
{
  "name": "comparison table",
  "class": "custome_table",
  "tag": "section",
  "settings": [
    {
      "type": "color",
        "id": "section_background",
        "label": "Background Color"
    },
    {
        "type": "text",
        "id": "section_padding",
        "label": "Paddings (top right bottom left)",
        "info": "6rem 6rem 0 6rem"
      },
    
    {
      "type": "header",
      "content": "Content"
    },
    {
      "type": "text",
      "id": "section_heading",
      "label": "Section Heading"
    },
       {
      "type": "textarea",
      "id": "section_description",
      "label": "Section Description"
    },
    
    {
      "type": "header",
      "content": "Caption styles"
    },
    {
        "type": "color",
        "id": "Heading_color",
        "label": "Main-Heading Color"
    },
    {
        "type": "color",
        "id": "description_color",
        "label": "Section Description Color"
    },
        
        
     {
        "type": "color",
        "id": "unselected_border_color",
        "label": "Unselected Content Border Color",
        "default": "#393838"
    },
   {
        "type": "color",
        "id": "Selected_content_color",
        "label": "Selected Content Color",
        "default": "#393838"
    },
     {
        "type": "color",
        "id": "selected_border_color",
        "label": "Selected Content Border Color",
        "default": "#393838"
    },
    
    {
      "type": "header",
      "content": "Appearance"
    },
    {
      "type": "select",
      "id": "text_align",
      "label": "Text alignment",
      "default": "left",
      "info":"Center aligns below image on mobile",
      "options":
      [
        {
          "value": "left",
          "label": "Left"
        },
        {
          "value": "center",
          "label": "Center"
        },
        {
          "value": "right",
          "label": "Right"
        }
      ]
    },
    {
      "type": "range",
      "id": "heading_size",
      "min": 13,
      "max": 72,
      "step": 1,
      "unit": "px",
      "label": "Heading size",
      "default": 22
    },
    {
      "type": "range",
      "id": "text_size",
      "min": 13,
      "max": 72,
      "step": 1,
      "unit": "px",
      "label": "Text size",
      "default": 22
    },
    {
      "type": "color",
      "id": "cell_Primary_color",
